{
  "name": "@sudobility/di_web",
  "version": "0.1.107",
  "description": "Web implementations of dependency injection services for Sudobility",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/sw/vite-plugin-service-worker.js",
      "types": "./dist/sw/vite-plugin-service-worker.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && cp src/sw/sw.js dist/sw/sw.js && cp src/sw/firebase-messaging-sw.js dist/sw/firebase-messaging-sw.js",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "prepublishOnly": "bun run clean && bun run test && bun run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "dependency-injection",
    "di",
    "web",
    "web3"
  ],
  "author": "Sudobility Inc",
  "license": "BUSL-1.1",
  "peerDependencies": {
    "@sudobility/components": "^5.0.11",
    "@sudobility/di": "^1.5.37",
    "@sudobility/types": "^1.9.52",
    "react": "^18.0.0 || ^19.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnqh/di_web.git"
  },
  "peerDependenciesMeta": {
    "@sudobility/subscription_lib": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@sudobility/components": "^5.0.10",
    "@sudobility/di": "^1.5.36",
    "@sudobility/subscription_lib": "^0.0.14",
    "@sudobility/types": "^1.9.51",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
