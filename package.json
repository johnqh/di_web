{
  "name": "@sudobility/di_web",
  "version": "0.1.2",
  "description": "Web implementations of dependency injection services for Sudobility",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "dependency-injection",
    "di",
    "web",
    "web3"
  ],
  "author": "Sudobility Inc",
  "license": "MIT",
  "peerDependencies": {
    "@sudobility/di": "^1.4.14",
    "@sudobility/types": "^1.9.5",
    "axios": "^1.13.0",
    "firebase": "^12.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/johnqh/di_web.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@sudobility/di": "^1.4.14",
    "@sudobility/types": "^1.9.5",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "^3.2.4",
    "axios": "^1.13.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "firebase": "^12.4.0",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
